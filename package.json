{
  "name": "pnpm-deduplicate",
  "type": "module",
  "version": "0.4.1",
  "packageManager": "pnpm@7.18.1",
  "description": "Deduplication tool for pnpm-lock.yaml files",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "pnpm-deduplicate": "bin/pnpm-deduplicate.mjs"
  },
  "files": [
    "README.md",
    "dist/*",
    "bin/*"
  ],
  "scripts": {
    "build": "pnpm build:esbuild && pnpm build:tsc",
    "build:esbuild": "node esbuild.mjs",
    "build:tsc": "tsc --build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@pnpm/lockfile-file": "^6.0.3",
    "@pnpm/lockfile-utils": "^5.0.0",
    "@pnpm/lockfile-walker": "^6.0.2",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/manifest-utils": "^4.1.2",
    "@pnpm/read-project-manifest": "^4.1.1",
    "@pnpm/types": "^8.9.0",
    "commander": "^9.4.1",
    "json5": "^2.2.1",
    "dependency-path": "^9.2.8",
    "path-exists": "^5.0.0",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@ocavue/eslint-config": "^0.4.5",
    "@types/node": "^18.11.11",
    "@types/semver": "^7.3.13",
    "esbuild": "^0.15.18",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.29.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
